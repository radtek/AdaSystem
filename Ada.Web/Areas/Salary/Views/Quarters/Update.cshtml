
@model Ada.Core.ViewModel.Wages.QuartersView
@{
    ViewBag.Title = "编辑岗位";
    ViewBag.Bread = new List<SelectListItem>() { new SelectListItem() { Text = "岗位", Value = Url.Action("Index") } };

}
@Html.Partial("Bread")
<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox">
                <div class="ibox-title">
                    <h5>@ViewBag.Title <small>请保证信息的准确性.</small></h5>
                    <div class="ibox-tools">
                        <a class="collapse-link">
                            <i class="fa fa-chevron-up"></i>
                        </a>
                    </div>
                </div>
                <div class="ibox-content">
                    @Html.Partial("Edit", Model)
                </div>
            </div>
        </div>
    </div>
</div>


